{
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft": "Error",
      "Microsoft.Hosting.Lifetime": "Error",
      "IdentityServer4": "Error",
      "IdentityServer4.Validation.TokenRequestValidator": "Error",
      "IdentityServer4.Validation.TokenValidator": "Error",
      "IdentityServer4.Endpoints.UserInfoEndpoint": "Error",
      "IdentityServer4.Endpoints.AuthorizeEndpoint": "Error",
      "IdentityServer4.Validation.AuthorizeRequestValidator": "Error"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "maxTelemetryItemsPerSecond": 10,
        "excludedTypes": "Request",
        "enablePerformanceCountersCollection": false,
        "enableDependencyTracking": false
      }
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=mydatabase.db;"
  }
}
