{
  "AppStore": {
    "VerifyReceiptUrl": "{APP_STORE_VERIFY_RECEIPT_URL}",
    "VerifyReceiptSharedSecret": "{APP_STORE_VERIFY_RECEIPT_SHARED_SECRET}"
  },
  "GooglePlayStore": {
    "PackageName": "{GOOGLE_PLAY_STORE_PACKAGE_NAME}",
    "Certificate": {
      "Type": "{GOOGLE_PLAY_STORE_CERTIFICATE_TYPE}",
      "ProjectId": "{GOOGLE_PLAY_STORE_CERTIFICATE_PROJECT_ID}",
      "PrivateKeyId": "{GOOGLE_PLAY_STORE_CERTIFICATE_PRIVATE_KEY_ID}",
      "PrivateKey": "{GOOGLE_PLAY_STORE_CERTIFICATE_PRIVATE_KEY}",
      "ClientEmail": "{GOOGLE_PLAY_STORE_CERTIFICATE_CLIENT_EMAIL}",
      "ClientId": "{GOOGLE_PLAY_STORE_CERTIFICATE_CLIENT_ID}",
      "AuthUri": "{GOOGLE_PLAY_STORE_CERTIFICATE_AUTH_URI}",
      "TokenUri": "{GOOGLE_PLAY_STORE_CERTIFICATE_TOKEN_URI}",
      "AuthProviderX509CertUrl": "{GOOGLE_PLAY_STORE_CERTIFICATE_AUTH_PROVIDER_X509_CERT_URL}",
      "ClientX509CertUrl": "{GOOGLE_PLAY_STORE_CERTIFICATE_CLIENT_X509_CERT_URL}"
    }
  },
  "EmailService": {
    "FromAddress": "{EMAIL_SERVICE_FROM_ADDRESS}",
    "FromName": "{EMAIL_SERVICE_FROM_NAME}",
    "SendGridApiKey": "{EMAIL_SERVICE_SEND_GRID_API_KEY}"
  },
  "FatSecretIdentityServer": {
    "Address": "{FAT_SECRET_IDENTITY_SERVER_ADDRESS}",
    "GrantType": "{FAT_SECRET_IDENTITY_SERVER_GRANT_TYPE}",
    "ClientId": "{FAT_SECRET_IDENTITY_SERVER_CLIENT_ID}",
    "ClientSecret": "{FAT_SECRET_IDENTITY_SERVER_CLIENT_SECRET}",
    "Scope": "{FAT_SECRET_IDENTITY_SERVER_SCOPE}"
  },
  "FatSecret": {
    "Address": "{FAT_SECRET_ADDRESS}",
    "ConsumerKey": "{FAT_SECRET_CONSUMER_KEY}",
    "ConsumerSecret": "{FAT_SECRET_CONSUMER_SECRET}"
  },
  "Firebase": {
    "Type": "{FIREBASE_TYPE}",
    "ProjectId": "{FIREBASE_PROJECT_ID}",
    "PrivateKeyId": "{FIREBASE_PRIVATE_KEY_ID}",
    "PrivateKey": "{FIREBASE_PRIVATE_KEY}",
    "ClientEmail": "{FIREBASE_CLIENT_EMAIL}",
    "ClientId": "{FIREBASE_CLIENT_ID}",
    "AuthUri": "{FIREBASE_AUTH_URI}",
    "TokenUri": "{FIREBASE_TOKEN_URI}",
    "AuthProviderX509CertUrl": "{FIREBASE_AUTH_PROVIDER_X509_CERT_URL}",
    "ClientX509CertUrl": "{FIREBASE_CLIENT_X509_CERT_URL}"
  },
  "Statistics": {
    "Period": "{STATISTICS_PERIOD}",
    "MinimumDays": "{STATISTICS_MINIMUM_DAYS}"
  },
  "ConnectionStrings": {
    "DefaultConnection": "{DEFAULT_DATABASE_CONNECTION}",
    "BlobStorageConnection": "{BLOB_STORAGE_CONNECTION}"
  }
}